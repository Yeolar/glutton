namespace glutton.fbs;

table Query {
    traceid: string;
    key: string;
}

enum ResultCode:int {
    OK = 0,

    // WARNING      <1000

    // ERROR        >1000
    E_SOURCE__UNTRUSTED = 1001,
}

table Result {
    traceid: string;
    value: string;
    code: ResultCode;
}

